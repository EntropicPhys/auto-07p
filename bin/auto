#! /bin/sh
if [ -z "$AUTO_DIR" ]; then
    bindir=`dirname $0`
    AUTO_DIR=`dirname $bindir`
    export AUTO_DIR
fi

if test "`uname`" = "Darwin"; then
  if which pythonw > /dev/null; then
    exec pythonw $AUTO_DIR/python/auto/interactiveBindings.py ${1+"$@"};
  fi
fi
exec python $AUTO_DIR/python/auto/interactiveBindings.py ${1+"$@"};
